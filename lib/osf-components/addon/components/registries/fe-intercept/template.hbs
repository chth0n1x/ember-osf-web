<div data-test-fe-intercept local-class='fe-intercept'>
    <OsfLink
                data-analytics-name='View registration'
                data-test-view-button='{{@node.id}}'
                @route='resolve-guid'
                @models={{array @node.id}}
    >
        <Button>
            {{t 'registries.fe_intercept.bot_button'}}
        </Button>
    </OsfLink>
    {{moment-format this.registration.dateRegistered 'MMMM D, YYYY'}}
</div>