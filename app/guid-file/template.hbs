<div class='container' local-class='FileDetail_container {{if this.isMobile 'Mobile'}}' aria-label={{t 'file_detail.aria.file_detail_container'}} >
    <div class='col-lg-12 pull-left'>
        <div id='leftPanel' class='col-lg-11 pull-left' local-class='FileDetail_leftPanel {{if this.isMobile 'Mobile'}}'>
            <div id='mainPanel' class='col-lg-12 pull-left' local-class='FileDetail_main pull-left {{if this.isMobile 'Mobile'}}'>
                <div class='row'>
                    <div local-class='FileDetail_project_link_div {{if this.isMobile 'Mobile'}}' class='col-lg-12'>
                        <a
                            data-test-project-link
                            data-analytics-name='Linked project'
                            aria-label={{t 'file_detail.aria.project_link'}}
                            href='#'
                        >
                            <div local-class='FileDetail_project-link {{if this.isMobile 'Mobile'}}'>
                                <h3 aria-label={{t 'file_detail.aria.file_project'}} local-class='FileDetail_file-project-h3 {{if this.isMobile 'Mobile'}}'>
                                    {{t 'file_detail.file_project_placeholder'}}
                                    {{this.model.parentFolder}}
                                </h3>
                            </div>
                        </a>
                    </div>
                </div>
                <div class='row' local-class='FileDetail_filename-buttons'>
                    <div class='col-lg-10'>
                        <div local-class='FileDetail_file-name {{if this.isMobile 'Mobile'}}'>
                            <h2 aria-label={{t 'file_detail.aria.file_name'}} local-class='FileDetail_file-name-h2 {{if this.isMobile 'Mobile'}}'>
                                {{this.model.name}}
                            </h2>
                        </div>
                    </div>
                    <div class='col-lg-2'>
                        <div class='btn btn-group'>
                            <ResponsiveDropdown
                                aria-label={{t 'file_detail.aria.more_buttons'}}
                                data-test-more-button
                                data-analytics-scope='Files'
                                as |dd|
                            >
                                <dd.trigger
                                    aria-label={{t 'file_detail.aria.more_buttons_trigger'}}
                                    data-test-more-button-trigger
                                    data-analytics-name='Toggle more buttons'
                                    local-class='FileDetail_dropdown-trigger'
                                >
                                    <Button
                                        local-class='FileDetail_dropdown-trigger-button'
                                    >
                                        <FaIcon @icon='ellipsis-v' @fixedWidth={{true}} />
                                    </Button>
                                </dd.trigger>
                                <dd.content
                                    local-class='FileDetail_dropdown-list'
                                >
                                    <div local-class='FileDetail_main-download-button'>
                                        <BsButton
                                            aria-label={{t 'file_detail.aria.download_button'}}
                                            data-analytics-name='Download button'
                                            data-test-download-button
                                            @type='link'
                                            @bubble={{true}}
                                            @onClick={{'#'}}
                                        >
                                            <span>
                                                <FaIcon @icon='download' @fixedWidth={{true}} />
                                                {{t 'general.download'}}
                                            </span>
                                        </BsButton>
                                    </div>
                                    <div local-class='FileDetail_main-share-button'>
                                        <BsButton
                                            aria-label={{t 'file_detail.aria.share_button'}}
                                            data-analytics-name='Share button'
                                            data-test-share-button
                                            @type='link'
                                            @bubble={{true}}
                                            @onClick={{'#'}}
                                        >
                                            <span>
                                                <FaIcon @icon='share-alt' @fixedWidth={{true}} />
                                                {{t 'general.share'}}
                                            </span>
                                        </BsButton>
                                    </div>
                                </dd.content>
                            </ResponsiveDropdown>
                        </div>
                    </div>
                </div>
                <div class='row'>
                    <div id='mfrIframeParent' local-class='FileDetail-file-render {{if this.isMobile 'Mobile'}}' aria-label={{t 'file_detail.aria.file_viewer'}} class='col-lg-12'>
                        <p>{{t 'file_detail.file_viewer_placeholder'}}</p>
                    </div>
                </div>
            </div>
            <div class='row'>
                <div hidden id='versions' class='pull-right' local-class='FilesDetail_Version-slide {{if this.isMobile 'Mobile'}}'>
                    <BsButton
                        data-test-close-versions
                        aria-label={{t 'file_detail.aria.close_revisions'}}
                        data-analytics-name='Close revision sidebar'
                        @type='link'
                        @onClick={{this.close}}
                        class='pull-right'
                        local-class='FilesDetail_Version-slide-close'
                    >
                        <FaIcon @icon='times' />
                    </BsButton>
                    <div local-class='FileDetail_version-header' aria-label={{t 'file_detail.aria.version_slide_header'}}>
                        <h2>{{t 'general.revisions'}}</h2>
                    </div>
                    <hr>
                    <table local-class='FileDetail_Version-table' aria-label={{t 'file_detail.aria.version_table'}}>
                        <tbody>
                            <tr>
                                <td local-class='FileDetaiil_revisions-tableVersionNumber' aria-label={{t 'file_detail.aria.version_number'}}>
                                    {{t 'file_detail.version.version_number'}}
                                </td>
                                <td local-class='FileDetaiil_revisions-tableDateCreated' aria-label={{t 'file_detail.aria.version_date_created'}}>
                                    {{t 'file_detail.version.date_created'}}
                                </td>
                                <td>
                                    <ResponsiveDropdown
                                        aria-label={{t 'file_detail.aria.version_more_buttons'}}
                                        data-test-more-buttons
                                        data-analytics-scope='Files'
                                        as |dd|
                                    >
                                        <dd.trigger
                                            aria-label={{t 'file_detail.aria.version_more_buttons_trigger'}}
                                            data-test-detail-trigger
                                            data-analytics-name='Toggle more button options'
                                            local-class='FileDetail_dropdown-trigger'
                                        >
                                            <Button
                                                data-analytics-name='File detail trigger'
                                                data-test-detail-trigger-button
                                                local-class='FileDetail_dropdown-trigger-button'
                                                @layout='fake-link'
                                            >
                                                <FaIcon @icon='caret-down' @fixedWidth={{true}} />
                                            </Button>
                                        </dd.trigger>
                                        <dd.content
                                            data-test-detail-content
                                            data-analytics-name='File detail dropdown'
                                            local-class='FileDetail_dropdown-content'
                                        >
                                            <table>
                                                <thead>
                                                    <tr>
                                                        <th>
                                                            {{t 'file_detail.version.user'}} {{this.model.file.user}}
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td>
                                                            <BsButton
                                                            aria-label={{t 'file_detail.aria.copy_md5'}}
                                                                data-test-copy-md5
                                                                data-analytics-name='Copy md5 button'
                                                                @bubble={{true}}
                                                                @onClick={{this.copy}}
                                                            >
                                                                <span>
                                                                    <FaIcon @icon='copy' @fixedWidth={{true}} />
                                                                    {{t 'file_detail.version.copy_md5'}}
                                                                </span>
                                                            </BsButton>
                                                        </td>
                                                        <td>
                                                            <i class='fa fa-info-circle'></i>
                                                            <span>
                                                                {{fa-icon 'info-circle'}}
                                                                <BsTooltip>
                                                                    {{t 'file_detail.md5_description'}}
                                                                </BsTooltip>
                                                            </span>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <BsButton
                                                                aria-label={{t 'file_detail.aria.copy_sha2'}}
                                                                data-test-copy-sha2
                                                                data-analytics-name='Copy sha-2 button'
                                                                @bubble={{true}}
                                                                @onClick={{this.copy}}
                                                            >
                                                                <span>
                                                                    <FaIcon @icon='copy' @fixedWidth={{true}} />
                                                                    {{t 'file_detail.version.copy_sha2'}}
                                                                </span>
                                                            </BsButton>
                                                        </td>
                                                        <td>
                                                            <i class='fa fa-info-circle'></i>
                                                            <span>
                                                                {{fa-icon 'info-circle'}}
                                                                <BsTooltip>
                                                                    {{t 'file_detail.sha2_description'}}
                                                                </BsTooltip>
                                                            </span>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <BsButton
                                                                aria-label={{t 'file_detail.aria.download_button'}}
                                                                data-test-download-button
                                                                data-analytics-name='Download button'
                                                                @bubble={{true}}
                                                                @onClick={{this.download}}
                                                            >
                                                                <span>
                                                                    <FaIcon @icon='download' @fixedWidth={{true}} />
                                                                </span>
                                                                {{t 'general.download'}}
                                                            </BsButton>
                                                        </td>
                                                        <td>
                                                            <div class='col-lg-1'>
                                                                {{t 'file_detail.version.download_count'}}
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </dd.content>
                                    </ResponsiveDropdown>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <hr>
                </div>
            </div>
            <div class='row'>
                <div hidden id='tags' class='pull-right' local-class='FilesDetail_Tags-slide {{if this.isMobile 'Mobile'}}'>
                    <BsButton
                        aria-label={{t 'file_detail.aria.close_tags'}}
                        data-analytics-name='Close tags slide'
                        data-test-close-tags
                        @type='link'
                        @onClick={{this.close}}
                        class='pull-right'
                        local-class='FilesDetail_Tags-slide-close'
                    >
                        <FaIcon @icon='times' />
                    </BsButton>
                    <h2>
                        {{t 'general.tags'}}
                    </h2>
                    <hr>
                    <div local-class='container-fluid FileDetail_tags-container'>
                        <p>
                            <a href='#'>{{t 'general.science'}}</a>
                        </p>
                        <p>
                            <a href='#'>{{t 'general.engineering'}}</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div local-class='FileDetail_navbar-right {{if this.isMobile 'Mobile col-lg-12 pull-right'}}' class='col-lg-1 pull-right'>
            <div class='col-lg-12' local-class='FileDetail_navbar-buttons {{if this.isMobile 'Mobile'}}'>
                <div local-class='FilesDetail_versions-button-div {{if this.isMobile 'Mobile'}}'>
                    <BsButton
                        data-test-versions-button
                        data-analytics-name='Versions slide button'
                        class='button btn'
                        local-class='FilesDetail_versions-button {{if this.isMobile 'Mobile'}}'
                        @size='lg'
                        @onClick={{this.toggleVersions}}
                    >
                        <FaIcon @icon='history' @fixedWidth={{true}}/>
                    </BsButton>
                </div>
                <div local-class='FilesDetail_tags-button-div {{if this.isMobile 'Mobile'}}'>
                    <BsButton
                        data-test-tags-button
                        data-analytics-name='Tags slide button'
                        class='button btn'
                        local-class='FilesDetail_tags-button {{if this.isMobile 'Mobile'}}'
                        @size='lg'
                        @onClick={{this.toggleTags}}
                    >
                        <FaIcon @icon='tags' @fixedWidth={{true}}/>
                    </BsButton>
                </div>
            </div>
        </div>
    </div>
</div>
